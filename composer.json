{
    "name": "jasonlbeggs/tailwind-preset",
    "description": "A Tailwind CSS preset for Laravel.",
    "keywords": [
        "laravel",
        "laravel preset",
        "preset",
        "tailwind",
        "tailwindcss"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jason Beggs",
            "email": "jasonlbeggs@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "laravel/framework": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "Jasonlbeggs\\TailwindPreset\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jasonlbeggs\\TailwindPreset\\TailwindServiceProvider"
            ]
        }
    }
}